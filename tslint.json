{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "semicolon": false,
    "quotemark": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "member-access": false,
    "no-console": false,
    "arrow-parens": false,
    "jsx-no-lambda": false,
    "max-classes-per-file": false,
    "jsx-no-multiline-js": false,
    "max-line-length": false,
    "no-empty": false,
    "jsx-alignment": false
  }
}
